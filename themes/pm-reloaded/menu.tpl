<!-- ======================= -->
<header class="navbar-light navbar-sticky header-static">
	<nav class="navbar navbar-expand-lg">
		<div class="container">
			<a class="navbar-brand" href="{$link}/homepage">
				{$title}
			</a>
			<button class="navbar-toggler ms-auto" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
		        <span class="text-body h6 d-none d-sm-inline-block">Menu</span>
			    <span class="navbar-toggler-icon"></span>
		    </button>
			<div class="collapse navbar-collapse" id="navbarCollapse">
				<ul class="navbar-nav navbar-nav-scroll mx-auto">
                    <!-- Homepage -->
                    <li class="nav-item"><a class="nav-link" href="{$link}/homepage">{$l_homepage}</a></li>
                    <li class="nav-item dropdown dropdown-fullwidth">
                        <!-- Categories & Articles -->
					    <a class="nav-link dropdown-toggle" href="#" id="menu_articles" data-bs-toggle="dropdown" aria-haspopup="false" aria-expanded="false">{$l_articles} <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-chevron-down" viewBox="0 0 16 16">
                            <path fill-rule="evenodd" d="M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708z"/>
                        </svg></a>
						<div class="dropdown-menu" aria-labelledby="menu_articles">
							<div class="container">
								<div class="row g-4 p-3 flex-fill">
                                {foreach $menu_articles_req as $mar}
									<div class="col-sm-6 col-lg-3">
										<div class="card bg-transparent">
											<img class="card-img rounded" src="{$mar.img}" alt="{$mar.title}">
											<div class="card-body px-0 pt-3">
												<h6 class="card-title mb-0"><a href="{$link}/article/{$mar.id}" class="btn-link text-reset fw-bold">{$mar.title}</a></h6>
												<ul class="nav nav-divider align-items-center text-uppercase small mt-2">
													<li class="nav-item">
														<a href="{$link}/space/{$mar.author}" class="text-reset btn-link">{$mar.author}</a>
													</li>
													<li class="nav-item">{$mar.datep|date_format:"%d/%m/%y"}</li>
												</ul>
											</div>
										</div>
									</div>
                                {/foreach}
								</div>
								<div class="row px-3">
									<div class="col-12">
										<ul class="list-inline mt-3">
											<li class="list-inline-item">{$l_categories}:</li>
											<li class="list-inline-item"><a href="{$link}/articles" class="btn btn-sm btn-danger-soft">{$l_allarticles}</a></li>
                                            {foreach $menu_categories_req as $mcr}
											    <li class="list-inline-item"><a href="{$link}/category/{$mcr.title}" class="btn btn-sm btn-success-soft">{$mcr.title}</a></li>
                                            {/foreach}
										</ul>
									</div>
								</div>
							</div>
						</div>
					</li>
                    {foreach $menu_pages_req as $mpr}
                        <li class="nav-item">
                            <a class="nav-link" href="{$link}/page/{$mpr.id}">{$mpr.title}</a>
                        </li>
                    {/foreach}
				</ul>
			</div>
			<div class="nav flex-nowrap align-items-center">
				<div class="nav-item d-none d-md-block">
                {if !isset($smarty.session.id)}
					<a href="{$link}/register" class="btn btn-sm btn-danger mb-0 mx-2">{$l_register}</a>
                    <a href="{$link}/login" class="btn btn-sm btn-danger mb-0 mx-2">{$l_login}</a>
                {else}
                    {foreach $menu_users_req as $mur}
                        <a href="{$link}/space/{$ur.username}" class="btn btn-sm btn-danger mb-0 mx-2">{$l_myspace}</a>
                        <a href="{$link}/logout" class="btn btn-sm btn-danger mb-0 mx-2">{$l_logout}</a>
                    {/foreach}
                    {foreach $menu_ranks_req as $mrr}
                        {if $mrr.p_panelaccess == 1}
                            <a href="{$link}/panel/index" class="btn btn-sm btn-danger mb-0 mx-2">{$l_panel}</a>
                        {/if}
                    {/foreach}
                {/if}
				</div>
			</div>
		</div>
	</nav>
	<section class="py-2">
		<div class="container">
			<div class="row g-0">
				<div class="col-12 bg-primary-soft p-2 rounded">
					<div class="d-sm-flex align-items-center text-center text-sm-start">
						<!-- Title -->
						<div class="me-3">
							<span class="badge bg-primary p-2 px-3">{$l_announce}</span>
						</div>
						<!-- Announce -->
						<div> <a href="#" class="text-reset btn-link">{$l_welcometo} {$title}.</a></div>
					</div>
				</div>
			</div>
		</div>
	</section>
</header>